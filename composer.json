{
    "name": "javck/ezlaravel",
    "description": "Easy learning guide to  Laravel + Voyager",
    "type": "liberary",
    "keywords": ["ezlaravel","laravel","admin","panel","voyager"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/javck/package_ezlaravel/issues",
        "source": "https://github.com/javck/package_ezlaravel"
    },
    "authors": [
        {
            "name": "zack lin",
            "email": "javck.zack@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^9.0",
        "laravelcollective/html": "^6.2",
        "tcg/voyager": "1.5.*",
        "laravel/socialite": "^5.0.3"
    },
    "require-dev":{
    },
    "autoload": {
        "psr-4": {
            "Javck\\Ezlaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Javck\\Ezlaravel\\EzlaravelServiceProvider",
                "Collective\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Ezlaravel": "Javck\\Ezlaravel\\Facades\\Ezlaravel",
                "Form" : "Collective\\Html\\FormFacade",
                "Html" : "Collective\\Html\\HtmlFacade",
                "Input" : "Illuminate\\Support\\Facades\\Input",
                "Collection" : "Illuminate\\Database\\Eloquent\\Collection"
            }
        }
    }
}
